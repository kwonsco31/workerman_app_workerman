<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:state_pressed="true">
        <layer-list
          xmlns:android="http://schemas.android.com/apk/res/android">
            <item>
                <shape>
                    <solid android:color="@color/color_group_c_translate" />
                </shape>
            </item>
            <item android:left="1.0px" android:top="1.0px" android:right="1.0px" android:bottom="1.0px">
                <shape android:shape="rectangle"
                  xmlns:android="http://schemas.android.com/apk/res/android">
                    <solid android:color="@color/color_group_dddddd" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_pressed="false">
        <layer-list
          xmlns:android="http://schemas.android.com/apk/res/android">
            <item>
                <shape>
                    <solid android:color="@color/common_trans_bluee" />
                </shape>
            </item>
            <item android:left="1.0px" android:top="1.0px" android:right="1.0px" android:bottom="1.0px">
                <shape android:shape="rectangle"
                  xmlns:android="http://schemas.android.com/apk/res/android">
                    <solid android:color="@color/color_group_dddddd" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>