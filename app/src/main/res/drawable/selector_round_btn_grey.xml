<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="false">
        <shape
          xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@color/grey_bfbfbf" />
            <stroke android:width="1.0dip" android:color="@color/grey_bfbfbf" />
            <corners android:radius="@dimen/common_round_layout_corners_2dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape
          xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@color/grey_trans_bfbfbf" />
            <stroke android:width="1.0dip" android:color="@color/grey_trans_bfbfbf" />
            <corners android:radius="@dimen/common_round_layout_corners_2dp" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape
          xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@color/common_grey_list_normal_color" />
            <stroke android:width="1.0dip" android:color="@color/color_group_dddddd" />
            <corners android:radius="@dimen/common_round_layout_corners_2dp" />
        </shape>
    </item>
</selector>