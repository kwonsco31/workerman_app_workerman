<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="true">
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@color/common_base_color_ef5aa2" />
            <stroke android:width="1.3dip" android:color="@color/common_base_color_ef5aa2" />
            <corners android:radius="@dimen/common_round_layout_corners" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@android:color/white" />
            <stroke android:width="1.3dip" android:color="@color/color_group_dddddd" />
            <corners android:radius="@dimen/common_round_layout_corners" />
        </shape>
    </item>

</selector>